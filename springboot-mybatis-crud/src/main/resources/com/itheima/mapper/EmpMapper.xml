<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.EmpMapper">

<!--    XML文件的名称与Mapper接口名称一致,并且放置在相同包下(同包同名)
    XML文件的namespace属性为Mapper接口全限定名一致。
    XML文件中sql语句的id与Mapper接口中的方法名一致。-->

<!-- resultType：单条记录所封装的类型   -->
    <select id="list" resultType="com.itheima.pojo.Emp">
        select * from emp where name like concat('%', #{name}, '%') and gender = #{gender}
            and entrydate between #{startDate} and #{endDate} order by update_time desc
    </select>

</mapper>